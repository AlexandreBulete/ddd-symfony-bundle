{
    "name": "alexandrebulete/ddd-symfony-bundle",
    "description": "Symfony Bundle for DDD Foundation - Messenger Command/Query Bus integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Bulete"
        }
    ],
    "require": {
        "php": "^8.2",
        "alexandrebulete/ddd-foundation": "^1.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Alexandrebulete\\DddSymfonyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alexandrebulete\\DddSymfonyBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

